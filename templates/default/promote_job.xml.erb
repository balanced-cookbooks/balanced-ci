<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <logRotator>
    <daysToKeep>-1</daysToKeep>
    <numToKeep>50</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.plugins.hipchat.HipChatNotifier_-HipChatJobProperty>
      <room></room>
      <startNotification>false</startNotification>
    </jenkins.plugins.hipchat.HipChatNotifier_-HipChatJobProperty>
      <hudson.model.ParametersDefinitionProperty>
        <parameterDefinitions>
          <hudson.model.StringParameterDefinition>
            <name>PACKAGE</name>
            <description>The name of package to promote, e.g. `balanced`</description>
            <defaultValue></defaultValue>
          </hudson.model.StringParameterDefinition>
          <hudson.model.StringParameterDefinition>
            <name>VERSION</name>
            <description>The version of package to promote, e.g. `1.3.4-1`</description>
            <defaultValue></defaultValue>
          </hudson.model.StringParameterDefinition>
        </parameterDefinitions>
      </hudson.model.ParametersDefinitionProperty>
  </properties>

  <%- if @builder_label -%>
  <assignedNode><%= @builder_label %></assignedNode>
  <canRoam>false</canRoam>
  <%- else -%>
  <canRoam>true</canRoam>
  <%- end -%>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash -xe
<%= @command %>
      </command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>

  </publishers>
  <buildWrappers>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper plugin="ansicolor@0.3.1">
      <colorMapName>xterm</colorMapName>
    </hudson.plugins.ansicolor.AnsiColorBuildWrapper>
    <% if @environment_script %><%= render '_build_wrappers/environment_script.erb', variables: {script: @environment_script} %><% end %>
  </buildWrappers>
</project>

LAST_TAGGED_GIT_REV=$(git rev-list --tags --max-count=1)
LAST_GIT_TAG=$(git describe $LAST_TAGGED_GIT_REV)
MOST_RECENT_GIT_REV=$(git rev-list HEAD | head -1)

GIT_TAG=''
HAS_GIT_TAG=false
if [ "${LAST_TAGGED_GIT_REV}" = "${MOST_RECENT_GIT_REV}" ]
then
   GIT_TAG=$LAST_GIT_TAG
   HAS_GIT_TAG=true
fi

echo LAST_TAGGED_GIT_REV=$LAST_TAGGED_GIT_REV
echo LAST_GIT_TAG=$LAST_GIT_TAG
echo MOST_RECENT_GIT_REV=$MOST_RECENT_GIT_REV
echo GIT_TAG=$GIT_TAG
echo HAS_GIT_TAG=$HAS_GIT_TAG

